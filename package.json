    "formidable": "^1.2.1",
    "postcss-nested": "^4.1.0",
    "postcss-nested-ancestors": "^2.0.0",
    "request": "^2.88.0",
    "vite": "^4.5.0",
    "vite-plugin-css-injected-by-js": "^3.3.0"
  },
  "dependencies": {
    "@codexteam/icons": "^0.0.6"
  },
  "publishConfig": {
    "access": "public",
