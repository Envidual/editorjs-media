  "keywords": [
    "codex editor",
    "media",
    "editor.js",
    "editorjs"
  ],
  "description": "Media (Image, Audio, Video) Tool for Editor.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/envidual/editorjs-media.git"
  },
  "files": [
